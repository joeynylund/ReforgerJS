{
  "server": {
    "id": 1,
    "name": "SERVER NAME",
    "host": "xxx.xxx.xxx.xxx",
    "queryPort": 1111,
    "rconPort": 1111,
    "rconPassword": "PASSWORD",
    "reloadCommandsOnStartup": true,
    "logReaderMode": "tail",
    "filename": "console.log",
    "backfill": true,
    "logDir": "CC:/path/to/reforger/log/folder",
    "sftp": {
      "host": "xxx.xxx.xxx.xxx",
      "port": 21,
      "username": "SFTP Username",
      "password": "SFTP Password"
    },
    "ftp": {
      "host": "xxx.xxx.xxx.xxx",
      "port": 21,
      "user": "FTP Username",
      "password": "FTP Password"
    }
  },
  "customParsers": {
    "rjs-logging": {
      "enabled": false,
      "logDir": "/path/to/RJSLogging/events"
    }
  },
  "consoleLogLevel": "info",
  "outputLogLevel": "info",
  "connectors": {
    "discord": {
      "token": "",
      "clientId": "",
      "guildId": ""
    },
    "mysql": {
      "enabled": false,
      "host": "mysql-host",
      "port": 3306,
      "username": "reforgerjs",
      "password": "password",
      "database": "reforgerjs",
      "dialect": "mysql"
    },
    "battlemetrics": {
      "enabled": true,
      "orgID": "Your Org ID",
      "serverID": "Your Server ID",
      "token": "Your BattleMetrics API Token",
      "listID": "Your Ban List ID"
    }
  },
  "roles": {
    "roleName": "discord RoleID",
    "roleName1": "discord RoleID",
    "roleName2": "discord RoleID",
    "roleName3": "discord RoleID"
  },
  "roleLevels": {
    "1": ["roleName", "roleName1"],
    "2": ["roleName2"],
    "3": ["roleName3"]
  },
  "commands": [
    {
      "command": "whois",
      "enabled": false,
      "commandLevel": 3
    },
    {
      "command": "stats",
      "enabled": false,
      "commandLevel": 3,
      "servers": 1,
      "statsTable": "Same_as_DBLogStats_Plugin"
    },
    {
      "command": "voteinfo",
      "commandLevel": 3,
      "enabled": false
    },
    {
      "command": "rcon",
      "enabled": false,
      "commandLevel": 3,
      "ban": 3,
      "shutdown": 3,
      "restart": 3,
      "kick": 3
    },
    {
      "command": "reload",
      "enabled": false,
      "commandLevel": 1
    },
    {
      "command": "killhistorywcs",
      "enabled": false,
      "commandLevel": 3
    },
    {
      "command": "messagehistorywcs",
      "enabled": false,
      "commandLevel": 3
    }
  ],
  "plugins": [
    {
      "plugin": "LogVoteKickVictim",
      "enabled": false,
      "channel": ""
    },
    {
      "plugin": "LogVoteKickStart",
      "enabled": false,
      "channel": ""
    },
    {
      "plugin": "DBLog",
      "enabled": false,
      "interval": 5
    },
    {
      "plugin": "RJS_LogChat",
      "requiredMods": "RJS Logging",
      "enabled": false,
      "channel": "your-discord-channel-id-here"
    },
    {
      "plugin": "RJS_LogTK",
      "requiredMods": "RJS Logging",
      "enabled": false,
      "channel": "your-discord-channel-id-here"
    },
    {
      "plugin": "RJS_DBEvents",
      "requiredMods": "RJS Logging",
      "enabled": false
    },
    {
      "plugin": "RJS_DBLog",
      "requiredMods": "RJS Logging",
      "enabled": false
    },
    {
      "plugin": "VoteLogs",
      "enabled": false
    },
    {
      "plugin": "DBLogStats",
      "enabled": false,
      "interval": 5,
      "path": "C:/path/to/saves/profile/.save/playersave",
      "tableName": ""
    },
    {
      "plugin": "SeedTrackerBasic",
      "enabled": false,
      "interval": 5,
      "seedStart": 4,
      "seedEnd": 40
    },
    {
      "plugin": "AltChecker",
      "enabled": false,
      "channel": "",
      "logAlts": true,
      "logOnlyOnline": false
    },
    {
      "plugin": "ServerStatus",
      "enabled": false,
      "channel": "",
      "messageID": "",
      "interval": 1,
      "showFPS": true,
      "showMemoryUsage": false,
      "discordBotStatus": true,
      "embed": {
        "title": "Arma Reforger Server Status",
        "color": "#00FF00",
        "footer": "ReforgerJS",
        "thumbnail": false,
        "thumbnailURL": "https://IMAGE_URL_HERE.png"
      }
    }
  ],
  "github": {
    "owner": "ZSU-GG-Reforger",
    "repo": "ReforgerJS"
  }
}

